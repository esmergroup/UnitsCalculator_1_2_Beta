		JButton btnSupport = new JButton("Support");
		btnSupport.addActionListener(new ActionListener() {
			public void actionPerformed(ActionEvent arg0) {
				// Hier kommt die Verbindungsklasse :P
				JFrame support = new JFrame();
				support.setVisible(true);
				support.setTitle("Support");
				support.setResizable(false);
				support.setBounds(100, 100, 330, 240);
				contentPane = new JPanel();
				contentPane.setForeground(SystemColor.info);
				contentPane.setBackground(SystemColor.activeCaption);
				contentPane.setBorder(new EmptyBorder(5, 5, 5, 5));
				support.setContentPane(contentPane);
				contentPane.setLayout(null);
				
				JLabel lblNewLabel = new JLabel("Java2Enterprise Community");
				lblNewLabel.setForeground(new Color(255, 0, 0));
				lblNewLabel.setFont(new Font("Arial", Font.PLAIN, 14));
				lblNewLabel.setBounds(10, 11, 189, 22);
				contentPane.add(lblNewLabel);
				
